def is_mul(num):
    for i in range(1,int(num**0.5)+1):
        if num%i==0:
            if len(str(i))==3 and len(str(num//i))==3:
                return True
    return False
def largest_P(num):
    for i in range(num,1,-1):
        if str(i)==str(i)[::-1] and is_mul(i):
            return (i)
        
    
x=largest_P(999*999)
print(x)